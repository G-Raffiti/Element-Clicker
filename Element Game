//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////		
				//Magie, ICI tout ce qui est en Rapport avec les Pouvoirs ! Spell
//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
Resources 		
// Gestion du mana, des Cooldown etc c'est ICI
	*TEMPLATE
		hidden
	*maxenergie
		is always:100+((transfo3)*(transfo3))/2
	*autoclick
		on tick:yield -1 autoclick
	*delayprodx
		on tick:
			if (have transfope3)
			yield -1.5 delayprodx
			else
			yield -1 delayprodx
			end
		end
	*delayprodx2
		on tick:
			if (have transfope3)
			yield -1.5 delayprodx2
			else
			yield -1 delayprodx2
			end
		end
	*delayautox
		on tick:
			if (have transfope3)
			yield -1.5 delayautox
			else
			yield -1 delayautox
			end
		end
	*delayprodx3
		on tick:
			if (have transfope3)
			yield -1.5 delayprodx3
			else
			yield -1 delayprodx3
			end
		end
	*delaytimeiswater1
		on tick:yield -1 delaytimeiswater1
	*TEMPLATE
//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
Upgrades 		
//Les Spells ! Les Pouvoirs ! Les Sortilèges ! Les Capacités Spécials etc...
	*TEMPLATE
		on click:anim glow
		on click:anim icon wobble
		tag:spell
		no text
		no buy
		class:noBackground
	*timeiswater
		name:Le Temps c'est de l'Eau
		icon:icons[2,2]
		on click:
			$cost=(idltime/4)
			if (delaytimeiswater1=0)
				yield -$cost idltime
				yield ($cost*2) water
				toast Vous avez gagné [($cost*2)] Eau.
				yield 60 delaytimeiswater1
			else
				toast Attendez [delaytimeiswater1] secondes.
			end
		end
		desc:Produit de l'Eau en consommant du Temps <//><b>Effect:</b><.> Delay : [delaytimeiswater1] sec<.>Coût : [(idltime/4)] Unitées de Temps<.>Gains : [((idltime/4)*2)] Eau
		req:waterstatu>=5
				
	*prodx
		name:Boost 60 Secondes
		icon:icons[9,2]
		on click:
			$cost=50
			if (delayprodx=0)
				if (energie>=$cost)
					yield -$cost energie
					yield (60*golds:ps) golds
					yield (60*technos:ps) technos
					yield (60*earths:ps) earths
					yield (60*fires:ps) fires
					yield (60*waters:ps) waters
					yield (60*airs:ps) airs
					toast Vous avez gagnez 60 sec de vos productions
					yield 40 delayprodx
				else if (energie<$cost)
					toast vous n'avez pas assez d'énergie !
				else
					toast BUG !!!
				end
			else
				toast Attendez [delayprodx] secondes.
			end
		end			
		desc:Produit 60 secondes de toutes les productions<//><b>Effect:</b><.> Delay : [delayprodx] sec<.>cost : 50 Energie
		req:102 maxenergie
	*autox
		name:Auto Click Terre 30 sec
		icon:badges[8,3] icons[9,0]
		on click:
			$cost=100
			if (delayautox=0)
				if (energie>=$cost)
					yield -$cost energie
					yield 30 autoclick
					yield 60 delayautox
				else if (energie<$cost)
					toast vous n'avez pas assez d'énergie !
				else
					toast BUG !!!
				end
			else
				toast Attendez [delayautox] secondes.
			end
		end			
		desc: Click 1x par Seconde pendant 30 secondes.<//><b>Effect:</b><.> Delay : [delayautox] sec<.>cost : 100 Energie
		req:120 maxenergie
	*prodx2
		name:Big Boost
		icon:badges[8,3] icons[9,2]
		on click:
			$cost=(buildingtxts)
			if (delayprodx2=0)
				if (energie>=$cost)
					yield -$cost energie
					yield (5*(buildingtxts)*golds:ps) golds
					yield ((buildingtxts)*technos:ps) technos
					yield ((buildingtxts)*earths:ps) earths
					yield ((buildingtxts)*fires:ps) fires
					yield ((buildingtxts)*waters:ps) waters
					yield ((buildingtxts)*airs:ps) airs
					toast Vous avez gagnez [buildingtxts] sec de vos production
					yield 120 delayprodx2
				else if (energie<$cost)
					toast vous n'avez pas assez d'énergie !
				else
					toast BUG !!!
				end
			else
				toast Attendez [delayprodx2] secondes.
			end
		end			
		desc:Produit [buildingtxts] Secondes de toutes vos productions <//><b>Effect:</b><.> Delay : [delayprodx2] sec<.>cost : [buildingtxts] Energie
		req:150 maxenergie
	*prodx3
		name:Beaucoup d'or
		icon:icons[6,1]
		on click:
			$cost=500+(buildingtxts)
			if (delayprodx3=0)
				if (energie>=$cost)
					yield -$cost energie
					yield (100*(buildingtxts)*golds:ps) golds
					toast Vous avez gagnez [(100*(buildingtxts)*golds:ps)] Or
					yield 120 delayprodx3
				else if (energie<$cost)
					toast vous n'avez pas assez d'énergie !
				else
					toast BUG !!!
				end
			else
				toast Attendez [delayprodx3] secondes.
			end
		end			
		desc:Vraiment Beaucoup d'Or<//><b>Effect:</b><.> Delay : [delayprodx3] sec<.>cost : 500 Energie
		req:(maxenergie>=500) and (have transfope3)
	*TEMPLATE
