///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
//Le Trêfle Clover ICI
Shinies			
	*TEMPLATE
	*luckyButton
		on click:log Woop
		movement:onRight moveLeft fade
		frequency:60
		frequency variation:30
		icon:bigicons[6,3]
		class:bigButton
		on click:yield 1 luckclick
		on click:
			$boost=1
			$cl2=1
			$cl3=1
			if (have clover) $boost=4
			if (have clover2) $cl2=7
			if (have clover3) $cl3=10
			if (chance(25%))
				$amount=((7,random(golds:ps*60*1,golds:ps*60*3))*$boost*$cl2*$cl3)
				toast Le trefle à 4 feuilles vous octroie<//><b>[$amount] Or</b>.
				yield $amount golds
			else
				$amount=(3*$boost*$cl2*$cl3)
				toast Le trefle à 4 feuilles vous octroie<//><b>[$amount] Ressource(s) Élémentaire(s) [s?$amount]</b>!
				yield $amount water
				yield $amount earth
				yield $amount fire
				yield $amount air
			end
		end
	*TEMPLATE
