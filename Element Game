//Palier Counter
Includes
	*include buildcount %building
	*[%building]counter
		owned
		always hidden
		passive:multiply yield of [%building] by (1+(floor([%building]/25))*0.25)
Upgrades
	[i:buildcount %building="filon"]
	[i:buildcount %building="minea"]
	[i:buildcount %building="mineb"]
	[i:buildcount %building="firetemple"]
	[i:buildcount %building="firestatu"]
	[i:buildcount %building="earthtemple"]
	[i:buildcount %building="earthstatu"]
	[i:buildcount %building="transfo"]
	[i:buildcount %building="transfo2"]
	[i:buildcount %building="transfo3"]
	[i:buildcount %building="watertemple"]
	[i:buildcount %building="waterstatu"]
	[i:buildcount %building="airtemple"]
	[i:buildcount %building="airstatu"]
	
