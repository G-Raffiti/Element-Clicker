					name:[%upname] 5
					icon:badges[[%badge],0] icons[[%icon]] 
					cost:[%bost1]e2 [%res1], [%bost2]e2 [%res2], [%bost3]e2 [%res3], [%bost4]e2 [%res4], [%bost5]e2 [%res5]
					req:([%req0]*80)) and (have [%name]plus4)
